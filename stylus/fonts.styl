@font-face {
    font-family: 'fine';
    src: url('fonts/fine/com4f_rg-webfont.eot');
    src: url('fonts/fine/com4f_rg-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/fine/com4f_rg-webfont.woff2') format('woff2'),
         url('fonts/fine/com4f_rg-webfont.woff') format('woff'),
         url('fonts/fine/com4f_rg-webfont.ttf') format('truetype'),
         url('fonts/fine/com4f_rg-webfont.svg#com4t_fine_regularregular') format('svg');
    font-weight: 100;
    font-style: normal;

}

@font-face {
    font-family: 'existence';
